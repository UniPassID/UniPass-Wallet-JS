{
  "name": "unipass-wallet",
  "description": "Example of using ts-node in a TS monorepo",
  "version": "0.0.2",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "dist/index",
  "scripts": {
    "build": "pnpm run clean && pnpm run compile",
    "test": "jest --testTimeout 30000",
    "clean": "rimraf -rf ./dist",
    "compile:esm": " tsc --module es2015 --outDir dist/esm -p tsconfig.build.json",
    "compile:cjs": " tsc --module commonjs --outDir dist/cjs -p tsconfig.build.json",
    "compile": "pnpm run compile:esm && pnpm run compile:cjs",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "ethers": "^5.6.9",
    "unipass-wallet-dkim": "^0.0.2"
  },
  "devDependencies": {
    "@jest/types": "~28.1.0",
    "@types/jest": "~27.5.0",
    "@types/node": "^18.0.5",
    "dotenv": "^16.0.1",
    "hardhat": "^2.10.1",
    "jest": "~28.1.0",
    "rimraf": "~3.0.2",
    "ts-jest": "~28.0.0",
    "typescript": "~4.7.0"
  }
}