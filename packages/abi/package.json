{
  "name": "unipass-wallet-abi",
  "version": "0.0.2",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run clean && pnpm run compile",
    "clean": "rimraf -rf ./dist",
    "compile:esm": " tsc --module es2015 --outDir dist/esm -p tsconfig.build.json",
    "compile:cjs": " tsc --module commonjs --outDir dist/cjs -p tsconfig.build.json",
    "compile": "pnpm run compile:esm && pnpm run compile:cjs",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "dkim": "github:zzhengzhuo/node-dkim#next",
    "ethers": "^5.6.9",
    "mailparser": "github:zzhengzhuo/mailparser"
  },
  "devDependencies": {
    "@types/node": "^18.0.5",
    "rimraf": "~3.0.2",
    "typescript": "~4.7.0"
  }
}